import java.util.*;
public class Series2
{
    public static void main(String[] args)
    {
        //write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        System.out.println(6+" ");
        int s=6;
        for (int i=1;i<n;i++)
        {
            s=s+5*i;
            System.out.println(s);
        }
    }
}
