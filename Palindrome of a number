import java.util.*;
public class Palindrome
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int rev=0,r,b;
        b=a;
        while(a>0)
        {
            r=a%10;
            rev=(rev*10)+r;
            a=a/10;
        }
        if(rev==b)
        {
            System.out.println("Palindrome");
        }
        else
        {
            System.out.println("Not a Palindrome");
        }
        }
    }
