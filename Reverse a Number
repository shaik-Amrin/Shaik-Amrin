import java.util.*;
public class Reverse
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int rev=0,r;
        while(a>0)
        {
            r=a%10;
            rev=(rev*10)+r;
            a=a/10;
        }
                System.out.println(rev);
            }
        }
    
