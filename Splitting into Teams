import java.util.*;
public class Split
{
    public static void main(String args[])
    {
    Scanner sc=new Scanner(System.in);
    int a,b,c,d;
    float r,i;
    a=sc.nextInt();
    b=sc.nextInt();
    c=a/b;
    d=a%b;
    System.out.printf("The number of students in each team is %d and left out is %d",c,d);
    
    }
}
