import java.util.*;
public class Harshard
{
    public static void main(String[] args)
    {
    Scanner sc=new Scanner(System.in);
    int b,c,d,e,f,g,h;
    int a=sc.nextInt();
    b=a%10;
    c=a/10;
    d=c%10;
    e=c/10;
    f=e%10;
    g=e/10;
    h=b+d+f+g;
    if(a%h==0)
    {
        System.out.println("Harshad Number");
    }
    else
    {
        System.out.println("Not Harshad Number");
        
       }
    }
}                                   
