import java.util.*;
public class Area
{
    public static void main(String args[])
    {
    Scanner sc=new Scanner(System.in);
    int s=sc.nextInt();
    int l=sc.nextInt();
    int b=sc.nextInt();
    float r=sc.nextFloat();
    int a1=s*s;
    int a2=l*b;
    double a3=3.14*r*r;
    System.out.printf("%d\n%d\n%.2f",a1,a2,a3);
    }
}
