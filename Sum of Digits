import java.util.*;
public class Sum
{
    public static void main(String[] args)
    {
    Scanner sc=new Scanner(System.in);
    int a,b;
    int c,d,e,g,f;
    a=sc.nextInt();
    b=a%10;
    c=a/10;
    d=c%10;
    e=c/10;
    f=e%10;
    g=e/10;
    //System.oout.println();
    //System.out.println(c);
    //System.out.println(d);
    //System.out.println(e);
    System.out.print(b+d+f+g);
    }
}
