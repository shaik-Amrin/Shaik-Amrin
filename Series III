import java.util.*;
public class Series3
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        double d=0;
        for(int i=1;i<=n;i++)
        {
            d=Math.pow(3,i);
            System.out.printf("%.0f ",d);
        }
    }
}
