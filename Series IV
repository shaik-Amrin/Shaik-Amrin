import java.util.*;
public class Series4
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        double s=0.5;
        System.out.println(s);
        for(int i=1;i<n;i++)
        {
            s=s*3;
            System.out.println(s);
        }
    }
}
