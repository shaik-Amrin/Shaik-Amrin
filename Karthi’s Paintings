import java.util.*;
public class Game
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        int e=sc.nextInt();
        int f=sc.nextInt();
        int x=(a+b)/2;
        int y=c+d;
        int z=e+f;
        if(z==x&&z==y)
        {
            System.out.println("Karthi can fix both painting");
        }
        else
        {
            System.out.println("Karthi cannot fix both painting");
        }
      }
  }//code here
