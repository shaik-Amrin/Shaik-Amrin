import java.util.*;
public class Series
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a;
        for(int i=1;i<=n;i++)
        {
            if(i%2==0)
            {
                a=(i*i)-2;
                System.out.println(a+ " ");
            }
            else 
            {
                a=(i*i)+2;
                System.out.println(a+" ");
            }
        }
    }
}
