import java.util.*;
public class Series9
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        long a=sc.nextLong();
        long s=2;
        System.out.print(2+" ");
        for(int i=1;i<=a;i++)
        {
            s=s*s-1;
            System.out.print(s +" ");
        }
    }
}
