import java.util.*;
public class Series6
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int s=0,s1=0;
        for(int i=1;i<=n;i++)
        {
            if(i%2==0)
            {
                s=(i*i)-2;
                System.out.println(s);
            }
            else
            {
                s1=(i*i)-1;
                System.out.println(s1);
            }
        }
    }
}
