import java.util.*;
public class String10
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        int s=0;
        for(char c:a.toCharArray())
        {
            c=Character.toLowerCase(c);
            if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u')
            {
                ++s;
            }
        } 
        System.out.println(s);
    }
}
