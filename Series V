import java.util.*;
public class Series5
{
    public static void main(String[] args)
    {
        //Write your code
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int s=11;
        System.out.println(s*s);
        for(int i=1;i<n;i++)
        {
            s=s+4;
            System.out.println(s*s);
        }
    }
}
